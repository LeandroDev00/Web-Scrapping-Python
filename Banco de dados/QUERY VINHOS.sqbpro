SELECT *
FROM PRODUTOS --SELEÇÃO DE TODOS OS REGISTROS

SELECT *
FROM PRODUTOS
WHERE PRECO BETWEEN '0' AND '50'
ORDER BY PRECO ASC --SELEÇÃO DE PRODUTOS DE R$0,00 Á R$50,00 EM ORDEM CRESCENTE DOS PRECOS

SELECT * 
FROM PRODUTOS 
WHERE PRECO BETWEEN '50' AND '300'
ORDER BY PRECO ASC  --SELEÇÃO DE PRODUTOS DE R$50,00 Á R$300,00 EM ORDEM CRESCENTE DOS PRECOS

SELECT *, COUNT(PRECO) AS QTD_PRECO
FROM PRODUTOS
WHERE PRECO &gt; '0'
GROUP BY PRECO
HAVING COUNT(PREÇO)
ORDER BY PRODUTO --SELEÇÃO DE PRODUTOS E A CONTAGEM DOS 
