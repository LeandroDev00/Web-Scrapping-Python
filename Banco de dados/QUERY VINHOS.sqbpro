CREATE TABLE IF NOT EXISTS USUARIO(
	IDUsuario INTEGER PRIMARY KEY AUTOINCREMENT,
	NOME TEXT NOT NULL,
	EMAIL TEXT NOT NULL ,
	SENHA TEXT NOT NULL,
	CONFIRMASENHA TEXT NOT NULL
);

ALTER TABLE USUARIO
ADD CONFIRMASENHA INTEGER NOT NULL;

--CRIAÇÃO DE TABELA MERCADORIA PARA REGISTRO DE PRODUTOS
CREATE TABLE IF NOT EXISTS PRODUTOS(
	IDProduto INTEGER PRIMARY KEY AUTOINCREMENT,
	PRODUTO TEXT ,
	PRECO REAL ,
	TIPO TEXT,
	REGIAO TEXT ,
	GRADUACAO DECIMAL 
);

